        </sectionGroup>
      </sectionGroup>
    </sectionGroup>
  </configSections>
  <dataConfiguration defaultDatabase="DefaultConnection"/>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Server=localhost;Database=NombreBaseDatos;User Id=usuario;Password=contraseña;TrustServerCertificate=True;MultipleActiveResultSets=true" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="ApiBaseUrl" value="https://localhost:7147"/>
    <add key="TicApiUrl" value="https://localhost:7147"/>
    <add key="Proveedor:BaseUrl" value="https://api.Proveedor.com"/>
    <add key="Proveedor:AuthUrl" value="https://Proveedor.com"/>
    <add key="Proveedor:TokenEndpoint" value="/auth/Proveedor/protocol/openid-connect/token"/>
    <add key="Proveedor:SubscriptionUrl" value="/suscripciones"/>
    <add key="Proveedor:AdhesionUrl" value="/suscripciones"/>
    <add key="Proveedor:PaymentAndSubscriptionUrl" value="/pagos"/>
    <add key="Proveedor:PaymentUrl" value="https://Proveedor.com/checkout"/>
    <add key="Proveedor:GrantType" value="password"/>

